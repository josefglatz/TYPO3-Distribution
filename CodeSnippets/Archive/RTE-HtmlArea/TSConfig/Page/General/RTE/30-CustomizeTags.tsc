// <table>: prevent style, width, height attribute if the editor drag the table in RTE.
RTE.default.proc.entryHTMLparser_db.tags.table {
	fixAttrib.style.unset = 1
	fixAttrib.width.unset = 1
	fixAttrib.height.unset = 1
}
