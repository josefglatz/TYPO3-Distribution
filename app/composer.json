{
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		},
		{
			"type": "vcs",
			"url": "https://github.com/w8tcha/CKEditor-WordCount-Plugin"
		},
		{
			"type":"package",
			"package": {
				"name": "ckeditor/ckeditor-dev",
				"version":"4.7.0",
				"source": {
					"url": "https://github.com/ckeditor/ckeditor-dev",
					"type": "git",
					"reference":"release/4.7.x"
				}
			}
		}
	],
	"name": "josefglatz/typo3-distribution",
	"description": "A distribution package for TYPO3 CMS in composer mode with helhum/typo3-console and custom application environment support",
	"keywords": [
		"TYPO3 CMS",
		"typo3cms-distribution",
		"sitepackage",
		"typo3distribution",
		"typo3 example",
		"theme",
		"web"
	],
	"_comment": [
		"This is my TYPO3 distribution",
		"@TODO/TBD: https://github.com/helhum/TYPO3-Distribution/blob/master/composer.json#L49",
		"mimimi",
		"mumumu"
	],
	"authors": [
		{
			"name": "Josef Glatz",
			"email": "j.glatz@supseven.at",
			"role": "Developer",
			"homepage": "http://www.sup7even.digital"
		},
		{
			"name": "Josef Glatz",
			"email": "jousch@jousch.com",
			"role": "Developer",
			"homepage": "http://jousch.com"
		}
	],
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/josefglatz/TYPO3-Distribution/issues"
	},
	"minimum-stability": "beta",
	"config": {
		"optimize-autoloader": true,
		"secure-http": true,
		"bin-dir": "."
	},
	"require": {
		"php": "^7",
		"slowprog/composer-copy-file": "~0.2",
		"w8tcha/CKEditor-WordCount-Plugin": "dev-master#e70499b0873f745098b1a569efd6fb31103b9d51",
		"typo3/cms": "dev-master as 8.7.4",
		"typo3/cms-about": "dev-master as 8.7.4",
		"typo3/cms-belog": "dev-master as 8.7.4",
		"typo3/cms-beuser": "dev-master as 8.7.4",
		"typo3/cms-context-help": "dev-master as 8.7.4",
		"typo3/cms-fluid-styled-content": "dev-master as 8.7.4",
		"typo3/cms-felogin": "dev-master as 8.7.4",
		"typo3/cms-filelist": "dev-master as 8.7.4",
		"typo3/cms-filemetadata": "dev-master as 8.7.4",
		"typo3/cms-form": "dev-master as 8.7.4",
		"typo3/cms-impexp": "dev-master as 8.7.4",
		"typo3/cms-info": "dev-master as 8.7.4",
		"typo3/cms-info-pagetsconfig": "dev-master as 8.7.4",
		"typo3/cms-linkvalidator": "dev-master as 8.7.4",
		"typo3/cms-lowlevel": "dev-master as 8.7.4",
		"typo3/cms-recycler": "dev-master as 8.7.4",
		"typo3/cms-reports": "dev-master as 8.7.4",
		"typo3/cms-rsaauth": "dev-master as 8.7.4",
		"typo3/cms-rte-ckeditor": "dev-master as 8.7.4",
		"typo3/cms-scheduler": "dev-master as 8.7.4",
		"typo3/cms-setup": "dev-master as 8.7.4",
		"typo3/cms-sys-action": "dev-master as 8.7.4",
		"typo3/cms-sys-note": "dev-master as 8.7.4",
		"typo3/cms-taskcenter": "dev-master as 8.7.4",
		"typo3/cms-tstemplate": "dev-master as 8.7.4",
		"typo3/cms-viewpage": "dev-master as 8.7.4",
		"apache-solr-for-typo3/solr": "7.0.x-dev as 7.0.0",
		"causal/image_autoresize": "dev-master",
		"cpsit/mask-export": "dev-master as 0.9.1",
		"dmitryd/typo3-dd-googlesitemap": "^2.1",
		"dmitryd/typo3-realurl": "dev-master",
		"fluidtypo3/vhs": "dev-master",
		"georgringer/logging": "dev-master",
		"georgringer/deprecationloganalyzer": "dev-master as 2.0.2",
		"georgringer/news": "dev-master",
		"georgringer/page_speed": "dev-master",
		"helhum/typo3-console": "dev-master",
		"mask/mask": "dev-master as 3.0.0",
		"minifranske/gravatar": "dev-master",
		"reelworx/rx-shariff": "dev-master",
		"spooner-web/be_secure_pw": "dev-master",
		"t3monitor/t3monitoring_client": "dev-master"
	},
	"require-dev": {
		"ckeditor/ckeditor-dev": "4.7.0",
		"macopedia/mess_detector": "dev-master",
		"maxserv/yaml_configuration": "dev-master",
		"pyrech/composer-changelogs": "dev-master",
		"sgalinski/lfeditor": "dev-master"
	},
	"suggest": {
		"apache-solr-for-typo3/solr": "7.0.x-dev as 7.0.0",
		"causal/extractor": "dev-master",
		"cpsit/mask-export": "dev-master as 0.9.1",
		"cedricziel/transladder": "dev-master",
		"dl/yag": "dev-master",
		"georgringer/deprecationloganalyzer": "dev-master as 2.*",
		"GeorgRinger/maildebug": "dev-master",
		"georgringer/autoswitchtolistview": "dev-master",
		"gridelementsteam/gridelements": "dev-master",
		"helhum/typoscript-rendering": "dev-master",
		"ichhabrecht/content-defender": "dev-master as 2.2.3",
		"in2code/in2publish_core": "dev-master",
		"in2code/powermail": "dev-master",
		"josefglatz/beuser-fastswitch": "dev-master",
		"koninklijke-collective/my-redirects": "dev-master",
		"mask/mask": "dev-master as 3.0.0",
		"rlmp/rlmp-language-detection": "dev-master",
		"sjbr/sr-language-menu": "dev-master",
		"sup7even/benefits": "dev-master",
		"t3g/blog": "dev-master"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		},
		"helhum/typo3-console": {
			"install-binary": false,
			"install-extension-dummy": false
		},
		"copy-file": {
			"vendor/w8tcha/CKEditor-WordCount-Plugin/wordcount/": "web/typo3conf/ext/theme/Resources/Public/RTE/Plugins/wordcount/"
		}
	},
	"autoload": {
		"classmap": [
		],
		"psr-4": {
			"JosefGlatz\\Theme\\": "web/typo3conf/ext/theme/Classes",
			"JosefGlatz\\ThemeProject\\": "web/typo3conf/ext/theme/Classes"
		}
	},
	"scripts": {
		"clean-generated-files": [
			"rm -rf ./web/typo3conf/PackageStates.php  || true"
		],
		"package-states": [
			"@php ./typo3cms install:generatepackagestates"
		],
		"folder-structure": [
			"@php ./typo3cms install:fixfolderstructure"
		],
		"setup-active-extensions": [
			"@php ./typo3cms extension:setupactive"
		],
		"toolchain-install": [
			"./sgc-core/install.sh --local && cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-update": [
			"cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-watch": [
			"./sgc build && ./sgc"
		],
		"toolchain-watch-silent": [
			"./sgc build && ./sgc -s"
		],
		"toolchain-watch-nobrowsersync": [
			"./sgc build && ./sgc watch"
		],
		"toolchain-build": [
			"./sgc build"
		],
		"php-cs-fixer": [
			"php-cs-fixer fix --config ./Build/.php_cs"
		],
		"pre-deploy": [
			"./sgc build"
		],
		"post-autoload-dump": [
			"@package-states",
			"@folder-structure"
		],
		"post-install-cmd": [
			"SlowProg\\CopyFile\\ScriptHandler::copy"
		],
		"post-update-cmd": [
			"SlowProg\\CopyFile\\ScriptHandler::copy"
		]
	}
}
